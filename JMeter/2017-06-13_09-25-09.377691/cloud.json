{
 "local-bzt-version": "1.9.1", 
 "scenarios": {
  "simple": {
   "requests": [], 
   "data-sources": {}, 
   "script": "blazedemo_easy_peasy.jmx"
  }
 }, 
 "execution": [
  {
   "scenario": "simple", 
   "ramp-up": "1m", 
   "locations": {
    "us-central1-a": 1
   }, 
   "concurrency": 500, 
   "executor": "jmeter", 
   "hold-for": "25m"
  }
 ], 
 "modules": {
  "gatling": {
   "properties": {
    "gatling.data.file.bufferSize": 256
   }
  }, 
  "grinder": {
   "properties": {
    "grinder.useConsole": false
   }
  }, 
  "jmeter": {
   "properties": {
    "jmeter.save.saveservice.autoflush": "true", 
    "jmeterengine.force.system.exit": "true", 
    "jmeter.save.saveservice.connect_time": "true", 
    "summariser.name": "", 
    "mode": "Stripped"
   }
  }
 }, 
 "services": [
  {
   "run-at": "local", 
   "local": [
    {
     "metrics": [
      "cpu", 
      "mem", 
      "bytes-sent", 
      "bytes-recv", 
      "disk-read", 
      "disk-write", 
      "disk-space", 
      "engine-loop", 
      "conn-all"
     ]
    }
   ], 
   "module": "monitoring"
  }
 ]
}
