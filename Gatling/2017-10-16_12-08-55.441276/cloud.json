{
 "local-bzt-version": "1.9.5", 
 "scenarios": {
  "BlazeDemo.scala": {
   "keepalive": true, 
   "simulation": "BlazeDemo", 
   "script": "BlazeDemo.scala"
  }
 }, 
 "execution": [
  {
   "locations": {
    "us-central1-a": 1
   }, 
   "scenario": "BlazeDemo.scala", 
   "executor": "gatling"
  }
 ], 
 "modules": {
  "gatling": {
   "version": "2.2.4", 
   "properties": {
    "gatling.data.file.bufferSize": 256
   }
  }, 
  "grinder": {
   "properties": {
    "grinder.useConsole": false
   }
  }, 
  "jmeter": {
   "properties": {
    "jmeter.save.saveservice.autoflush": "true", 
    "jmeterengine.force.system.exit": "true", 
    "jmeter.save.saveservice.connect_time": "true", 
    "summariser.name": "", 
    "mode": "Stripped"
   }
  }
 }, 
 "services": [
  {
   "run-at": "local", 
   "local": [
    {
     "metrics": [
      "cpu", 
      "mem", 
      "bytes-sent", 
      "bytes-recv", 
      "disk-read", 
      "disk-write", 
      "disk-space", 
      "engine-loop", 
      "conn-all"
     ]
    }
   ], 
   "module": "monitoring"
  }
 ]
}
