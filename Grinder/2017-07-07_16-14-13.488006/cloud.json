{
 "local-bzt-version": "1.9.3", 
 "scenarios": {
  "properties": {
   "grinder.runs": 0
  }, 
  "script_sample": {
   "properties-file": null, 
   "script": "grinder_sample_ramp.py"
  }
 }, 
 "execution": [
  {
   "scenario": "script_sample", 
   "ramp-up": "10m", 
   "locations": {
    "us-west-1": 1
   }, 
   "concurrency": 500, 
   "executor": "grinder", 
   "locations-weighted": false, 
   "hold-for": "5m"
  }
 ], 
 "modules": {
  "gatling": {
   "properties": {
    "gatling.data.file.bufferSize": 256
   }
  }, 
  "grinder": {
   "properties": {
    "grinder.useConsole": false
   }
  }, 
  "jmeter": {
   "properties": {
    "jmeter.save.saveservice.autoflush": "true", 
    "jmeterengine.force.system.exit": "true", 
    "jmeter.save.saveservice.connect_time": "true", 
    "summariser.name": "", 
    "mode": "Stripped"
   }
  }
 }, 
 "services": [
  {
   "run-at": "local", 
   "local": [
    {
     "metrics": [
      "cpu", 
      "mem", 
      "bytes-sent", 
      "bytes-recv", 
      "disk-read", 
      "disk-write", 
      "disk-space", 
      "engine-loop", 
      "conn-all"
     ]
    }
   ], 
   "module": "monitoring"
  }
 ]
}
